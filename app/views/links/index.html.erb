  <%= link_to 'my flub needs a fix', edit_link_path(link), class: 'btn btn-outline-info btn-xs'%>
  <%= link_to 'toss a flub', link, method: :delete, data: { confirm: 'bye-bye flub?' }, class: 'btn btn-outline-info btn-xs' %>
<% end %>
</div>
